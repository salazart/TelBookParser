<?xml version="1.0" encoding="UTF-8"?> 
<Configuration> 
  <Appenders> 
	<File name="FileLogger" fileName="TelBookParser.log" append="true"> 
      <PatternLayout pattern="%d %t %-5p %c{2} - %m%n"/> 
    </File> 
  </Appenders> 
  <Loggers> 
    <Logger name="Logging" level="info"/> 
    <Root level="trace"> 
      <AppenderRef ref="FileLogger"/> 
    </Root> 
  </Loggers> 
</Configuration>